{
  "name": "@zendesk/garden-react-components",
  "description": "Shared React components",
  "license": "UNLICENSED",
  "repository": "zendeskgarden/react-components",
  "version": "13.14.0",
  "main": "./lib",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "pre-commit": "lint-staged",
    "build": "gulp",
    "changelog": "offline-github-changelog > CHANGELOG.md",
    "eslint": "eslint --ignore-pattern '/lib/' --ignore-pattern '/styleguide/build/' .",
    "gh-pages": "styleguidist build && gh-pages -d styleguide",
    "jest": "jest -i",
    "start": "styleguidist server",
    "stylelint": "stylelint `find src -name '*.css'`",
    "test": "jest --watch",
    "travis:before_deploy": "styleguidist build",
    "travis:gh-pages": "./publish-gh-pages.js",
    "travis:script": "npm run stylelint && npm run eslint && npm run jest",
    "preversion": "git pull && npm run travis:script",
    "version": "npm run build && git add -A lib",
    "postversion": "npm run postversion:changelog && npm run postversion:push",
    "postversion:changelog": "npm run changelog && git add CHANGELOG.md && git commit --allow-empty -m 'Update changelog'",
    "postversion:push": "git push origin master:master && git push --tags"
  },
  "dependencies": {
    "append-styles": "^1.1.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.5.8",
    "react-is-deprecated": "^0.1.2",
    "react-virtualized": "^9.9.0",
    "tabbable": "^1.0.6",
    "uuid": "^2.0.2 || ^3.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "@zendesk/garden-css-arrows": "1.0.1",
    "@zendesk/garden-css-avatars": "1.0.1",
    "@zendesk/garden-css-buttons": "3.6.1",
    "@zendesk/garden-css-callouts": "1.1.1",
    "@zendesk/garden-css-forms": "3.4.2",
    "@zendesk/garden-css-labels": "1.1.0",
    "@zendesk/garden-css-menus": "5.0.1",
    "@zendesk/garden-css-modals": "4.0.1",
    "@zendesk/garden-css-pagination": "1.1.0",
    "@zendesk/garden-css-tables": "^1.0.5",
    "@zendesk/garden-css-tabs": "3.2.0",
    "@zendesk/garden-css-tooltips": "2.0.2",
    "@zendesk/garden-css-variables": "2.1.0",
    "@zendesk/garden-stylelint-config": "^6.0.0",
    "@zendesk/garden-svg-icons": "2.4.0",
    "adler32": "0.1.7",
    "array-uniq": "1.0.3",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.23.0",
    "babel-standalone": "6.24.0",
    "chance-generators": "2.1.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-config-pretty-standard": "0.2.2",
    "file-loader": "0.11.1",
    "gh-pages": "0.12.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-clean": "0.3.2",
    "gulp-clean-css": "3.0.4",
    "gulp-postcss": "6.4.0",
    "gulp-rename": "1.2.2",
    "gulp-transform": "1.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "19.0.2",
    "json-loader": "0.5.4",
    "lint-staged": "4.0.3",
    "offline-github-changelog": "^1.1.2",
    "postcss": "5.2.17",
    "postcss-cssnext": "2.10.0",
    "postcss-import": "8.1.0",
    "postcss-inline-svg": "^3.0.0",
    "postcss-input-range": "4.0.0",
    "postcss-loader": "1.3.3",
    "postcss-modules": "0.6.4",
    "pre-commit": "1.2.2",
    "prettier": "1.5.3",
    "prettier-package-json": "1.4.0",
    "querystring": "0.2.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-styleguidist": "5.1.6",
    "react-svg-loader": "1.1.1",
    "sinon": "2.1.0",
    "stylelint": "^8.2.0",
    "stylelint-order": "^0.7.0",
    "unexpected": "10.27.0",
    "unexpected-check": "1.12.0",
    "unexpected-dom": "^4.0.0",
    "unexpected-react": "3.5.2",
    "unexpected-sinon": "10.7.1",
    "url-loader": "0.5.8",
    "webpack": "2.4.1"
  },
  "keywords": [
    "components",
    "react"
  ],
  "publishConfig": {
    "registry": "https://zdrepo.jfrog.io/zdrepo/api/npm/npm"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(gif)$": "<rootDir>/test/fileMock.js",
      "\\.(css)$": "<rootDir>/test/styleMock.js",
      "test/expect": "<rootDir>/test/unexpected-with-plugins.js",
      "react-addons-test-utils": "react-dom/test-utils.js"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/index.js",
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules|lib)/"
    ],
    "testRegex": "\\.*(spec\\.js)$"
  },
  "lint-staged": {
    "linters": {
      "!(lib)/**/*.js": [
        "eslint ./src",
        "prettier --write",
        "git add"
      ],
      "package.json": [
        "prettier-package-json --write",
        "git add"
      ],
      "*.css": [
        "stylelint"
      ]
    },
    "globOptions": {
      "matchBase": true,
      "nonegate": true
    }
  },
  "pre-commit": "pre-commit"
}
